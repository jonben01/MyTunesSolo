<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="268.0" prefWidth="270.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dk.easv.Jonas_MyTunesSolo.GUI.Controller.NewSongController">
   <children>
      <VBox layoutX="71.0" layoutY="44.0" prefHeight="210.0" prefWidth="128.0">
         <children>
            <TextField fx:id="txtTitle" prefHeight="14.0" prefWidth="128.0">
               <VBox.margin>
                  <Insets bottom="15.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="txtArtist">
               <VBox.margin>
                  <Insets bottom="55.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="txtFilePath" editable="false">
               <VBox.margin>
                  <Insets bottom="15.0" />
               </VBox.margin></TextField>
            <TextField fx:id="txtDuration" promptText="duration" />
         </children>
      </VBox>
      <Text layoutX="39.0" layoutY="57.0" strokeType="OUTSIDE" strokeWidth="0.0" text="title" />
      <Text layoutX="28.0" layoutY="105.0" strokeType="OUTSIDE" strokeWidth="0.0" text="artist" />
      <Text layoutX="28.0" layoutY="181.0" strokeType="OUTSIDE" strokeWidth="0.0" text="file path" />
      <Button fx:id="btnFileChooser" layoutX="189.0" layoutY="211.0" mnemonicParsing="false" onAction="#btnHandleFileChooser" text="filechooser" />
      <Button fx:id="btnMenuAddSong" layoutX="13.0" layoutY="224.0" mnemonicParsing="false" onAction="#btnHandleMenuAddSong" text="ADD" />
      <ComboBox fx:id="cmbGenre" layoutX="13.0" layoutY="123.0" prefWidth="150.0" />
      <Text layoutX="31.0" layoutY="20.0" strokeType="OUTSIDE" strokeWidth="0.0" text="genre, should be drop down options" />
      <Button fx:id="btnAddGenre" layoutX="189.0" layoutY="122.0" mnemonicParsing="false" onAction="#btnHandleAddGenre" text="Add genre" />
   </children>
</AnchorPane>
