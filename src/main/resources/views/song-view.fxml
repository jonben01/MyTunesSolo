<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="254.0" prefWidth="218.0" styleClass="anchor-pane" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dk.easv.Jonas_MyTunesSolo.GUI.Controller.NewSongController">
   <children>
      <Button fx:id="btnFileChooser" layoutX="117.0" layoutY="173.0" mnemonicParsing="false" onAction="#btnHandleFileChooser" prefHeight="26.0" prefWidth="89.0" text="Choose file..." />
      <Button fx:id="btnMenuAddSong" layoutX="11.0" layoutY="215.0" mnemonicParsing="false" onAction="#btnHandleMenuAddSong" text="Add to library" />
      <ComboBox fx:id="cmbGenre" layoutX="13.0" layoutY="95.0" prefHeight="26.0" prefWidth="150.0" promptText="select a genre" />
      <Button fx:id="btnAddGenre" layoutX="174.0" layoutY="95.0" mnemonicParsing="false" onAction="#btnHandleAddGenre" prefHeight="26.0" prefWidth="30.0" text="+" />
      <TextField fx:id="txtTitle" layoutX="13.0" layoutY="17.0" prefHeight="26.0" prefWidth="150.0" promptText="Insert title" />
      <TextField fx:id="txtArtist" layoutX="13.0" layoutY="51.0" promptText="Insert Artist(s) or group" />
      <TextField fx:id="txtFilePath" editable="false" layoutX="14.0" layoutY="135.0" prefHeight="26.0" prefWidth="192.0" promptText="File path" />
      <TextField fx:id="txtDuration" editable="false" layoutX="12.0" layoutY="173.0" prefHeight="26.0" prefWidth="86.0" promptText="duration" />
      <Button fx:id="btnCancelSong" layoutX="155.0" layoutY="214.0" mnemonicParsing="false" onAction="#btnHandleCancelSong" prefHeight="26.0" prefWidth="51.0" text="Cancel" />
      <Text layoutX="178.0" layoutY="35.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Title" />
      <Text layoutX="179.0" layoutY="68.0" strokeType="OUTSIDE" strokeWidth="0.0" text="By" />
      <Button fx:id="btnEditSong" layoutX="13.0" layoutY="215.0" mnemonicParsing="false" onAction="#btnHandleEditSong" text="Edit in library" />
   </children>
</AnchorPane>
